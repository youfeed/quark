{
  "name": "cubeshop",
  "version": "0.0.13",
  "description": "Mobile UI Components built on Web Components.",
  "main": "src/packages/index.js",
  "module": "src/packages/index.js",
  "unpkg": "src/packages/index.js",
  "scripts": {
    "checked": "npm run generate:file && tsc",
    "dev": "npm run checked && vite --open --force",
    "build": "npm run checked && vite build --config vite.config.build.ts && npm run generate:types",
    "serve": "vite preview",
    "add": "node cs/createComponentMode.js",
    "generate:file": "node cs/generate-csui.js",
    "generate:types": "node cs/generate-types.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allan2coder/cubeshop.git"
  },
  "keywords": [
    "web components",
    "vue",
    "webpack",
    "vite"
  ],
  "files": [
    "src",
    "dist",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "sass": "^1.34.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.31",
    "@types/pinyin": "^2.8.3",
    "@vitejs/plugin-legacy": "^1.4.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-rc.6",
    "axios": "^0.21.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "highlight.js": "^10.3.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.0",
    "pinyin": "^2.10.2",
    "prettier": "^2.0.0",
    "standard-version": "^9.3.0",
    "swiper": "6.5.1",
    "transliteration": "^2.2.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.5",
    "vite": "^2.3.4",
    "vite-plugin-md": "^0.11.0",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
