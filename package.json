{
  "name": "cubeshop",
  "version": "0.0.22",
  "description": "Mobile UI Components built on Web Components.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.js",
  "files": [
    "dist/packages",
    "dist/index.js",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "tsc && vite --open --force",
    "build:site": "npm run checked && vite build && npm run copy",
    "copy": "node cs/copy.js",
    "serve": "vite preview",
    "add": "node cs/createComponentMode.js",
    "build:commonjs": "rimraf lib && cross-env BABEL_ENV=commonjs babel ./src/packages -d lib --source-maps",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel ./src/packages -d es --source-maps",
    "build:all": "npm run build:commonjs && npm run build:es",
    "pub": "npm run build:all && node cs/publish.js",
    "build": "npm run checked && vite build --config vite.config.build.ts && npm run generate:types",
    "generate:file": "node cs/generate-csui.js",
    "checked": "npm run generate:file && tsc",
    "generate:types": "node cs/generate-types.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allan2coder/cubeshop.git"
  },
  "keywords": [
    "web components",
    "vue",
    "webpack",
    "vite"
  ],
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.31",
    "@types/pinyin": "^2.8.3",
    "@vitejs/plugin-legacy": "^1.4.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-rc.6",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "highlight.js": "^10.3.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.0",
    "pinyin": "^2.10.2",
    "prettier": "^2.0.0",
    "sass": "^1.34.0",
    "standard-version": "^9.3.0",
    "swiper": "6.5.1",
    "transliteration": "^2.2.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.5",
    "vite": "^2.3.4",
    "vite-plugin-md": "^0.11.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.4",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
